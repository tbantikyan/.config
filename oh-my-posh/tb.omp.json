{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#f01a1a",
          "style": "plain",
          "template": "@{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#2462f2",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#b072f7",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#ff9500>git:</>{{ .HEAD }}</>",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " ERROR",
          "type": "status"
        },
        {
          "foreground": "#f01a1a",
          "type": "text",
          "style": "plain",
          "template": "\n‚ùØ"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
